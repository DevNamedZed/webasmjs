{
  "name": "webasmjs",
  "version": "0.1.0",
  "description": "TypeScript WebAssembly bytecode emitter and module builder",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && ts-node playground/build.ts",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:cover": "jest --coverage",
    "generate": "ts-node generator/index.ts",
    "build:playground": "ts-node playground/build.ts",
    "playground": "ts-node playground/build.ts && open playground/index.html",
    "prepare": "husky"
  },
  "bugs": {
    "url": "https://github.com/DevNamedZed/webasmjs/issues",
    "email": "devnamedzed@gmail.com"
  },
  "homepage": "https://github.com/DevNamedZed/webasmjs#readme",
  "author": {
    "name": "",
    "email": "devnamedzed@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevNamedZed/webasmjs"
  },
  "keywords": [
    "webasm",
    "wasm",
    "emit",
    "codegen",
    "webassembly",
    "typescript"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "esbuild": "^0.27.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}
